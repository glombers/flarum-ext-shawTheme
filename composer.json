{
  "name": "romanzpolski/flarum-ext-shawTheme",
  "title": "Shaw Academy",
  "description": "Custom Theme for Shaw Academy",
  "type": "flarum-extension",
  "require": {
    "flarum/core": "^0.1.0-beta.6"
